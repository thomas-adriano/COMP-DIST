package com.taxifleet.server.network;

import java.io.Serializable;

public interface NetworkNode extends Serializable, Cloneable {

	public void shout();

	public void sendNetAddress();

	public void startListeningTimers(long time);

	public NetworkNode getNextNode();
	
	public void setNextNode(NetworkNode node);
	
	public NetworkNode clone();
	
}
